<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >

    <link name="world"/>

    <link name="base_link">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://aster_description/meshes/ownStls/standBase.stl" scale="0.001 0.001 0.001"/>
            </geometry>        
            <material name="white"/>    
        </visual>
    </link>
    <!-- Base-->
    <link name="base_holder_link">
        <visual>
            <origin xyz="0.0 0.0 -0.02" rpy="0.0 0.0 ${-PI/2}"/>
            <geometry>
                <mesh filename="package://aster_description/meshes/subStuff/RohrHalter_v2.stl" scale="0.001 0.001 0.001"/>
            </geometry>         
            <material name="white"/>    
        </visual>
    </link>

    <link name="chest_bone_link">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://aster_description/meshes/Spine_Connector.stl" scale="0.001 0.001 0.001"/>
            </geometry>      
            <material name="white"/>       
        </visual>
    </link>

    <link name="chest_link">
        <visual>
            <origin xyz="0.0 0.0 -0.012" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://aster_description/meshes/Chest.stl" scale="0.001 0.001 0.001"/>
            </geometry>      
            <material name="blue"/>       
        </visual>
    </link>

    <link name="backbone_link">
        <visual>
            <origin xyz="-0.003 0.0 -0.085" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://aster_description/meshes/Spine.stl" scale="0.001 0.001 0.001"/>
            </geometry>      
            <material name="blue"/>       
        </visual>
    </link>
 
    <link name="hip_link">
        <visual>
            <origin xyz="-0.005 0.0 0.003" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://aster_description/meshes/Hips.stl" scale="0.001 0.001 0.001"/>
            </geometry>      
            <material name="blue"/>       
        </visual>
    </link>


   <!-- Head-->
    <link name="neck_link">
        <visual>
            <origin xyz="0.0 0.0 -0.012" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://aster_description/meshes/Head_Neck.stl" scale="0.001 0.001 0.001"/>
            </geometry>      
            <material name="blue"/>       
        </visual>
    </link>

    <link name="head_link">
        <visual>
            <origin xyz="0.0 0.0 -0.012" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://aster_description/meshes/Head_Back.stl" scale="0.001 0.001 0.001"/>
            </geometry>      
            <material name="white"/>       
        </visual>
    </link>
    <!-- Right Arm-->

    <!-- Right Gripper-->

    <!-- Left Arm-->

    <!-- Left Gripper-->

    <!-- Right Leg-->

    <!-- Left Leg-->


    <!-- Base Virtual Joints-->
    <joint name="virtual_1_joint" type="fixed">
        <parent link="world"/>
        <child link="base_link"/>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </joint>

    <joint name="virtual_2_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_holder_link"/>
        <origin xyz="0.0 0.0 0.65" rpy="0.0 0.0 0.0"/>
    </joint>

    <joint name="virtual_3_joint" type="fixed">
        <parent link="base_holder_link"/>
        <child link="chest_bone_link"/>
        <origin xyz="0.042 0.0 0.015" rpy="0.0 0.0 0.0"/>
    </joint>

    <joint name="virtual_4_joint" type="fixed">
        <parent link="chest_bone_link"/>
        <child link="chest_link"/>
        <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
    </joint>

    <joint name="virtual_5_joint" type="fixed">
        <parent link="chest_bone_link"/>
        <child link="backbone_link"/>
        <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
    </joint>

    <joint name="virtual_6_joint" type="fixed">
        <parent link="backbone_link"/>
        <child link="hip_link"/>
        <origin xyz="0 0 -0.2" rpy="0.0 0.0 0.0"/>
    </joint>


     <!--Head Joints-->
    <joint name="joint_1" type="revolute">
        <parent link="chest_link"/>
        <child link="neck_link"/>
        <axis xyz="0 0 1 "/>
        <limit lower="-1.57" upper="1.57" velocity="50.0" effort="1000.0"/>
        <origin xyz="0 0 0.1" rpy="0 0 0"/>
    </joint>

    <joint name="joint_2" type="revolute">
        <parent link="neck_link"/>
        <child link="head_link"/>
        <axis xyz="0 1 0"/>
        <limit lower="-0.5" upper="0.3" velocity="50.0" effort="1000.0"/>
        <origin xyz="0 0 0.01" rpy="0 0 0"/>
    </joint>

    <!-- Right Arm Joints-->

    <!-- Right Gripper Joints-->

    <!-- Left Arm Joints-->

    <!-- Left Gripper Joints-->

    <!-- Right Leg Joints-->

    <!-- Left Leg Joints-->
    
    
    
</robot>