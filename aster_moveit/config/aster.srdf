    <disable_collision link1="lh_gripper1_link" link2="lh_gripper1_pad_link" reason="Adjacent"/>
    <disable_collision link1="lh_gripper2_link" link2="lh_gripper2_pad_link" reason="Adjacent"/>

    <disable_collision link1="hip_link" link2="hip_rh_link" reason="Adjacent"/>
    <disable_collision link1="hip_rh_link" link2="knie_rh_link" reason="Adjacent"/>
